    <p class="d-flex align-items-center ">
      <span>
        <%= usuario.nombre %>
      </span>
      <button class="btn_menu ms-1" id="btn_menu">
        <i class="bi bi-list"></i>
      </button>
    <div class="menu_desplegable d-flex flex-column justify-content-between">
